    "babel-register": "^6.14.0",
    "css-loader": "^0.23.1",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "postcss-loader": "^0.10.1",
    "react-dom": "^15.3.1",
    "standard": "^8.0.0-beta.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7"
  }
