  "license": "ISC",
  "bugs": {
    "url": "https://github.com/madkraft/route_maps/issues"
  },
  "homepage": "https://github.com/madkraft/route_maps#readme",
  "dependencies": {
    "ampersand-app": "^2.0.0",
    "ampersand-router": "^4.0.0",
    "hjs-webpack": "^8.3.0",
    "leaflet": "^1.0.1",
    "normalize-css": "^2.3.1",
    "octicons": "^4.4.0",
    "react": "^15.3.1",
    "xhr": "^2.2.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "ava": "^0.16.0",
    "babel": "^6.5.2",
